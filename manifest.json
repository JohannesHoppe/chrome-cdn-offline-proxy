{
  "name": "CDN proxy",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "CDN proxy"
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest", "webRequestBlocking",
    "*://*.madcat/",
    "*://ajax.googleapis.com/"
  ],
  "web_accessible_resources": [
    "*"
  ]
}